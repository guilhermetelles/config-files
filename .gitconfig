[user]
      name = guilhermetelles
      email = guilherme.santos@linte.com.br

[alias]
      unstage = reset HEAD --
      last = log -1 HEAD
      st = status
      co = checkout
      cp = cherry-pick
      fdiff = diff --name-only
      fo = fetch origin
      br = branch
      ro = rebase origin/master
      mt = mergetool
      lg = log --color --pretty=format:'%Cred%h%Creset -%C(auto)%d %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
      ls = log --decorate=short --oneline
      cm = checkout master
[color]
  ui = auto
[color "branch"]
     current = yellow reverse
     local = yellow
     remote = green
[color "status"]
     added = yellow
     changed = green
     untracked = cyan
[push]
      default = simple
[rerere]
# Reuse recorded resolution
enabled = true
